{
  "version": 2,
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/test-imprimir-pdf/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "routes": [
    {
      "src": "/api/proxy/(.*)",
      "dest": "/api/proxy.js?path=$1"
    },
    {
      "src": "/api/bcra",
      "dest": "/api/bcra.js"
    },
    {
      "src": "/favicon.ico",
      "dest": "/test-imprimir-pdf/favicon.ico"
    },
    {
      "src": "/favicon.svg",
      "dest": "/test-imprimir-pdf/favicon.svg"
    },
    {
      "src": "/(.*)",
      "dest": "/test-imprimir-pdf/$1"
    }
  ]
}

