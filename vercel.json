{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "test-imprimir-pdf/dist",
  "installCommand": "npm install",
  "headers": [
    {
      "source": "/assets/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/test-imprimir-pdf/assets/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/proxy/:path*",
      "destination": "/api/proxy.js?path=:path*"
    },
    {
      "source": "/api/bcra",
      "destination": "/api/bcra.js"
    },
    {
      "source": "/api/auth",
      "destination": "/api/auth.js"
    },
    {
      "source": "/favicon.ico",
      "destination": "/favicon.ico"
    },
    {
      "source": "/favicon.svg",
      "destination": "/favicon.svg"
    },
    {
      "source": "/:path*",
      "destination": "/:path*"
    }
  ]
}

