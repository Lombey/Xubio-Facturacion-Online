{
  "version": 2,
  "routes": [
    {
      "src": "/api/proxy/(.*)",
      "dest": "/api/proxy.js?path=$1"
    },
    {
      "src": "/api/bcra",
      "dest": "/api/bcra.js"
    },
    {
      "src": "/favicon.ico",
      "dest": "/test-imprimir-pdf/favicon.ico"
    },
    {
      "src": "/favicon.svg",
      "dest": "/test-imprimir-pdf/favicon.svg"
    },
    {
      "src": "/(.*)",
      "dest": "/test-imprimir-pdf/$1"
    }
  ]
}

