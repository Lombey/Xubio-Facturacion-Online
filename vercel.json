{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "test-imprimir-pdf/dist",
  "installCommand": "npm install",
  "functions": {
    "api/**/*.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "headers": [
    {
      "source": "/assets/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/test-imprimir-pdf/assets/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/proxy/:path*",
      "destination": "/api/proxy.js?path=:path*"
    },
    {
      "source": "/api/auth",
      "destination": "/api/auth.js"
    }
  ]
}

